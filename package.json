{
  "name": "darcula-plus",
  "displayName": "Darcula Plus",
  "description": "Darcula Plus Theme",
  "version": "0.1.1",
  "type": "module",
  "publisher": "myziura",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Myziura/darcula-plus"
  },
  "watch": {
    "build": "src/theme.js",
    "extract": "src/extract.js"
  },
  "scripts": {
    "watch": "npm-watch",
    "build": "node src/build.js",
    "extract": "node --experimental-json-modules src/extract.js",
    "create-v": "vsce package",
    "publish": "vsce publish"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Darcula Plus",
        "uiTheme": "vs-dark",
        "path": "./themes/Darcula Plus.json"
      }
    ]
  },
  "devDependencies": {
    "npm-watch": "^0.11.0"
  }
}
